# <p1*/p2> ->
#   (<a> <b>) <p2>           (0 1 2) .
#   (<c> <d>) <p1>/<p2>      (3 4)   ; 
#             <p1>/<p1>/<p2> (5 6)   ,
#             <p1>/<p1>/<p2> (7)     ,
#             <p1>/<p1>/<p2> ()      .

(<a> <b>) <p2> (0 1 2) .
(<c> <d>) <p1> [ <p2> (3 4) ; 
                 <p1> [ <p2> (5 6), (7), () ] ] .


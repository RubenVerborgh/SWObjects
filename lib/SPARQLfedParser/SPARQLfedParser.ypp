%type <p_URI> _O_QGT_DTYPE_E_S_QIRIref_E_C IRIref PrefixedName GraphIRI
	      _QGraphIRI_E_Opt _Q_O_QIT_INTO_E_S_QIRIref_E_C_E_Opt

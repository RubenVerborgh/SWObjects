			 _Q_O_QIT_GRAPH_E_S_QVarOrIRIref_E_S_QGraphTemplate_E_S_QGT_DOT_E_Opt_S_QTriplesBlock_E_Opt_C_E_Star

%type <p_BasicGraphPattern> ConstructTemplate

%type <p_ArgList> ArgList _QArgList_E_Opt

%% /*** Grammar Rules ***/

 /*** BEGIN SPARQLfed - Change the grammar rules below ***/
Query:
    // Prologue ( Select | Construct | Describe | Ask | Replace | Update | Magage )
    Prologue _O_QSelect_E_Or_QConstruct_E_Or_QDescribe_E_Or_QAsk_E_Or_QReplace_E_Or_QUpdate_E_Or_QManage_E_C	{
	driver.root = $$ = $2;
    }
;

// Select | Construct | Describe | Ask | Replace | Update | Magage
_O_QSelect_E_Or_QConstruct_E_Or_QDescribe_E_Or_QAsk_E_Or_QReplace_E_Or_QUpdate_E_Or_QManage_E_C:
    Select	
  | Construct	
  | Describe	
  | Ask	
  | Replace	
  | Update	
  | Manage	
;

Update:
    Insert	
  | Delete	
  | Load	
  | Clear	
;

Replace:
    IT_REPLACE WhereClause IT_WITH GraphTemplate	{
	$$ = new Replace($2, $4);
    }
;

Insert:
    IT_INSERT GraphTemplate _QWhereClause_E_Opt	{
	$$ = new Insert($2, $3);
}
;

Delete:
    IT_DELETE GraphTemplate _QWhereClause_E_Opt	{
	$$ = new Delete($2, $3);
    }
;

Load:
    IT_LOAD _QIRIref_E_Plus _Q_O_QIT_INTO_E_S_QIRIref_E_C_E_Opt	{
	$$ = new Load($2, $3);
    }
;

Clear:
    IT_CLEAR _QGraphIRI_E_Opt	{
	$$ = new Clear($2);
    }
;

// IRIref+
_QIRIref_E_Plus:
    IRIref	{
	$$ = new ProductionVector<const URI*>($1);
    }
  | _QIRIref_E_Plus IRIref	{
	$1->push_back($2);
	$$ = $1;
    }
;

// 'INTO' IRIref
_O_QIT_INTO_E_S_QIRIref_E_C:
    IT_INTO IRIref	{
	$$ = $2;
    }
;

// ( 'INTO' IRIref )?
_Q_O_QIT_INTO_E_S_QIRIref_E_C_E_Opt:
    {
	$$ = NULL;
    }
  | _O_QIT_INTO_E_S_QIRIref_E_C	
;

GraphIRI:
    IT_GRAPH IRIref	{
	$$ = $2;
    }
;

_QGraphIRI_E_Opt:
    {
	$$ = NULL;
    }
  | GraphIRI	
;

Manage:
    Create	
  | Drop	
;

Create:
    IT_CREATE _QIT_SILENT_E_Opt GraphIRI	{
	$$ = new Create($2, $3);
    }
;

_QIT_SILENT_E_Opt:
    {
	$$ = SILENT_No;
    }
  | IT_SILENT	{
	$$ = SILENT_Yes;
}
;

Drop:
    IT_DROP _QIT_SILENT_E_Opt GraphIRI	{
	$$ = new Drop($2, $3);
}
;

Prologue:
    // BaseDecl? PrefixDecl*
    _QBaseDecl_E_Opt _QPrefixDecl_E_Star
;

// BaseDecl?
_QBaseDecl_E_Opt:

    | BaseDecl
;

// PrefixDecl*
_QPrefixDecl_E_Star:

    | _QPrefixDecl_E_Star PrefixDecl
;

BaseDecl:
    IT_BASE IRI_REF	{
	driver.setBase($2);
    }
;

PrefixDecl:
    IT_PREFIX {
	driver.ignorePrefix(true);
      } PNAME_NS {
	  driver.ignorePrefix(false);
      } IRI_REF	{
	  std::string prefix($3->getLexicalValue());
	  driver.addPrefix(prefix.substr(0, prefix.length()-1), $5);
      }
;

Select:
    // 'SELECT' ( 'DISTINCT' | 'REDUCED' )? ( Var+ | '*' ) DatasetClause* WhereClause SolutionModifier
    IT_SELECT _Q_O_QIT_DISTINCT_E_Or_QIT_REDUCED_E_C_E_Opt _O_QVar_E_Plus_Or_QGT_TIMES_E_C _QDatasetClause_E_Star WhereClause SolutionModifier	{
	$$ = new Select($2, $3, $4, $5, $6);
    }
;

// 'DISTINCT' | 'REDUCED'
_O_QIT_DISTINCT_E_Or_QIT_REDUCED_E_C:
    IT_DISTINCT	{
	$$ = DIST_distinct;
    }
  | IT_REDUCED	{
	$$ = DIST_reduced;
    }
;

// ( 'DISTINCT' | 'REDUCED' )?
_Q_O_QIT_DISTINCT_E_Or_QIT_REDUCED_E_C_E_Opt:
    {
	$$ = DIST_all;
    }
  | _O_QIT_DISTINCT_E_Or_QIT_REDUCED_E_C	
;

// Var+
_QVar_E_Plus:
    Var	{
	$$ = new POSList;
	$$->push_back($1);
    }
  | _QVar_E_Plus Var	{
	$1->push_back($2);
	$$ = $1;
    }
;

// Var+ | '*'
_O_QVar_E_Plus_Or_QGT_TIMES_E_C:
    _QVar_E_Plus	{
	$$ = $1;
    }
  | GT_TIMES	{
	$$ = new StarVarSet();
    }
;

// DatasetClause*
_QDatasetClause_E_Star:
    {
	$$ = new ProductionVector<const DatasetClause*>();
    }
  | _QDatasetClause_E_Star DatasetClause	{
	$1->push_back($2);
	$$ = $1;
    }
;

Construct:
    // 'CONSTRUCT' ConstructTemplate DatasetClause* WhereClause SolutionModifier
    IT_CONSTRUCT ConstructTemplate _QDatasetClause_E_Star WhereClause SolutionModifier	{
	/* $2 is known to be a DefaultGraphPattern because of grammar restrictions. */
	$$ = new Construct((DefaultGraphPattern*)$2, $3, $4, $5);
    }
;

Describe:
    // 'DESCRIBE' ( VarOrIRIref+ | '*' ) DatasetClause* WhereClause? SolutionModifier
    IT_DESCRIBE _O_QVarOrIRIref_E_Plus_Or_QGT_TIMES_E_C _QDatasetClause_E_Star _QWhereClause_E_Opt SolutionModifier	{
	$$ = new Describe($2, $3, $4, $5);
    }
;

// VarOrIRIref+
_QVarOrIRIref_E_Plus:
    VarOrIRIref	{
	$$ = new POSList;
	$$->push_back($1);
    }
  | _QVarOrIRIref_E_Plus VarOrIRIref	{
	$1->push_back($2);
	$$ = $1;
    }
;

// VarOrIRIref+ | '*'
_O_QVarOrIRIref_E_Plus_Or_QGT_TIMES_E_C:
    _QVarOrIRIref_E_Plus	{
	$$ = $1;
    }
  | GT_TIMES	{
	$$ = new StarVarSet();
    }
;

// 'WHERE'?
_QWhereClause_E_Opt:
    {
	$$ = NULL;
    }
  | WhereClause	
;

Ask:
    // 'ASK' DatasetClause* WhereClause
    IT_ASK _QDatasetClause_E_Star WhereClause	{
	$$ = new Ask($2, $3);
    }
;

DatasetClause:
    // 'FROM' ( DefaultGraphClause | NamedGraphClause )
    IT_FROM _O_QDefaultGraphClause_E_Or_QNamedGraphClause_E_C	{
	$$ = $2;
    }
;

// DefaultGraphClause | NamedGraphClause
_O_QDefaultGraphClause_E_Or_QNamedGraphClause_E_C:
    DefaultGraphClause	
  | NamedGraphClause	
;

DefaultGraphClause:
    SourceSelector	{
	$$ = new DefaultGraphClause($1, driver.posFactory);
    }
;

NamedGraphClause:
    IT_NAMED SourceSelector	{
	$$ = new NamedGraphClause($2, driver.posFactory);
    }
;

SourceSelector:
    IRIref	{

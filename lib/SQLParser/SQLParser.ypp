Create:
    IT_CREATE IT_TABLE Relation	{
	driver.curCreate = new sql::schema::Relation(*$3);
      } GT_LPAREN FieldOrKey _Q_O_QGT_COMMA_E_S_QFieldOrKey_E_C_E_Star GT_RPAREN _QParam_E_Star	{
	  driver.tables[*$3] = driver.curCreate;
	  delete $3;

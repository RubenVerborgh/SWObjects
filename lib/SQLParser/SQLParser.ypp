;

Insert:
    IT_INSERT IT_INTO Relation _QAttrList_E_Opt _O_QIT_VALUES_E_S_QConstList_E_Or_QSelectUnion_E_C	{
	driver.curCreate = new sql::schema::Relation(*$3);
